(program

(module Foo (imported) (exported () ()) (local () ())

  test.preamble()

  // Testing transferFrom(From,To,Value) when caller, From and To are distinct.
  // Overflow.
  // Recall that account 1 has 10 tokens and 2^256 (a 78 digit number) and is:
  // 115792089237316195423570985008687907853269984665640564039457584007913129639936

  (define x 115792089237316195423570985008687907853269984665640564039457584007913129639926)
  test.makeBalance(Foo.x)
  test.approve(9, Foo.x)
  test.makeCaller(9)           // switch to 9
  test.transferFrom(7, 1, Foo.x)

)

)
