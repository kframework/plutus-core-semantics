#!/usr/bin/env python2

import sys

def remove_comments_line(line):
    line = line.split(";")
    return line[0].rstrip()

orig = sys.stdin.readlines()
prelude = open("src/Prelude-supported.plc").readlines()

def flatten(xs):
    return sum(xs, [])

preprocessed_lines = map(remove_comments_line, flatten([[orig[0]], prelude[1:-1], orig[1:]]))
print("\n".join(preprocessed_lines))
